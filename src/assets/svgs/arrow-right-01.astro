---
import type { Svg } from "@lib/types";

type Props = Svg;

const {
  width = 20,
  height = 20,
  size,
  strokeWidth = 1.5,
  color = "currentColor",
  ...rest
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  width={size || width}
  height={size || height}
  color={color}
  fill="none"
  {...rest}
>
  <path
    d="M9.00005 6C9.00005 6 15 10.4189 15 12C15 13.5812 9 18 9 18"
    stroke="currentColor"
    stroke-width={strokeWidth}
    stroke-linecap="round"
    stroke-linejoin="round"></path>
</svg>
